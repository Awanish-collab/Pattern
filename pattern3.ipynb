   "source": [
    "# Print the following pattern for the given N number of rows.\n",
    "# Pattern for N = 4\n",
    "'''1\n",
    "   11\n",
    "   121\n",
    "   1221 '''"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "4\n",
      "1\n",
      "11\n",
      "121\n",
      "1221\n"
     ]
    }
   ],
   "source": [
    "n = int(input())\n",
    "for i in range(1,n+1):\n",
    "    for j in range(1,i+1):\n",
    "        if j == i or j == 1:\n",
    "            print('1',end='')\n",
    "        else:\n",
    "            print('2',end='')\n",
    "    print()"
   ]
  }
 ],
